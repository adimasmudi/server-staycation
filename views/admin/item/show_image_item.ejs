<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="show-tab"
>
  <div class="row">
    <% for (image of item.imageId){ %>
    <div class="col-md-4">
      <div class="card shadow py-4 mt-2">
        <div class="card-body">
          <img
            src="http://localhost:3000/<%= image.imageUrl %>"
            alt="image"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <%} %>
  </div>
  <a href="/admin/item" class="btn btn-warning mt-4 mb-4">Back</a>
</div>
